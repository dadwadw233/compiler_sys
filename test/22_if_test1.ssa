source_filename = "./22_if_test1.sy"
target datalayout = "e-m:e-p:64:64-i64:64-i128:128-n64-S128"

declare i64 @getint()

declare i64 @getch()

declare void @putint(i64)

declare void @putch(i64)

declare i64 @getarray(i64*)

declare i64 @putarray(i64, i64*)

define i64 @ifElse() {
entry:
  store i64 5, i64* undef
  %0 = load i64, i64* undef
  %1 = icmp eq i64 %0, 5
  %2 = zext i1 %1 to i64
  %3 = icmp ne i64 %2, 0
  br i1 %3, label %4, label %5

4:                                                ; preds = %entry
  store i64 25, i64* undef
  br label %8

5:                                                ; preds = %entry
  %6 = load i64, i64* undef
  %7 = mul i64 %6, 2
  store i64 %7, i64* undef
  br label %8

8:                                                ; preds = %5, %4
  %9 = load i64, i64* undef
  ret i64 %9
}

define i64 @main() {
entry:
  %0 = call i64 @ifElse()
  ret i64 %0
}
