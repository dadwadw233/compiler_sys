; ModuleID = 'sysyc'
source_filename = "./68_accumulate.sy"
target datalayout = "e-m:e-p:64:64-i64:64-i128:128-n64-S128"

declare i64 @getint()

declare i64 @getch()

declare void @putint(i64)

declare void @putch(i64)

declare i64 @getarray(i64*)

declare i64 @putarray(i64, i64*)

define i64 @main() {
entry:
  br label %0

0:                                                ; preds = %4, %entry
  %.01 = phi i64 [ 0, %entry ], [ %5, %4 ]
  %.0 = phi i64 [ 0, %entry ], [ %6, %4 ]
  %1 = icmp slt i64 %.0, 21
  %2 = zext i1 %1 to i64
  %3 = icmp ne i64 %2, 0
  br i1 %3, label %4, label %7

4:                                                ; preds = %0
  %5 = add i64 %.01, %.0
  %6 = add i64 %.0, 1
  br label %0

7:                                                ; preds = %0
  call void @putint(i64 %.01)
  ret i64 0
}
