; ModuleID = 'sysyc'
source_filename = "./47_minus_with_sub.sy"
target datalayout = "e-m:e-p:64:64-i64:64-i128:128-n64-S128"

declare i64 @getint()

declare i64 @getch()

declare void @putint(i64)

declare void @putch(i64)

declare i64 @getarray(i64*)

declare i64 @putarray(i64, i64*)

define i64 @main() {
entry:
  %0 = sub i64 0, 1
  %1 = sub i64 -2, %0
  %2 = add i64 -2, 1
  %3 = sub i64 0, %2
  %4 = sub i64 -2, 1
  %5 = sub i64 0, %4
  %6 = srem i64 %3, %5
  %7 = add i64 %1, %6
  call void @putint(i64 %7)
  ret i64 0
}
